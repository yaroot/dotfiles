
# A list of keys is in /usr/include/X11/keysym.h and in
# /usr/include/X11/keysymdef.h
# The XK_ is not needed.

"scrot -e 'mv $f ~/ 2>/dev/null'"
    Mod4 + Shift + x

"xscreensaver-command -lock"
    Mod4 + Shift + Alt + l

"dmenu_run `cat $HOME/.dmenurc`"
    Mod4 + p

"pwhash.sh -m"
    Mod4 + Shift + p

"urxvt"
    Mod4 + slash

#
# bspwm
#

"bspc quit"
    Mod4 + Shift + q

"bspc close"
    Mod4 + Shift + c

"bspc cycle_layout"
    Mod4 + t

"bspc rotate clockwise"
    Mod4 + r

"bspc rotate counter_clockwise"
    Mod4 + Shift + r

"bspc rotate full_cycle"
    Mod4 + u

"bspc toggle_floating"
    Mod4 + s

"bspc toggle_fullscreen"
    Mod4 + f

"bspc shift left"
    Mod4 + Shift + h

"bspc shift right"
    Mod4 + Shift + l

"bspc shift right"
    Mod4 + Shift + semicolon

"bspc shift up"
    Mod4 + Shift + k

"bspc shift down"
    Mod4 + Shift + j

"bspc nearest older"
    Mod4 + o

"bspc nearest newer"
    Mod4 + i

"bspc cycle next"
    Mod4 + j

"bspc cycle prev"
    Mod4 + k

"bspc cycle_desktop next --skip-free"
